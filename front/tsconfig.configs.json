    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.configs.tsbuildinfo",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "allowJs": true
  },
  "include": [
    "vite.config.ts",
    "postcss.config.js",
    "eslint.config.js",
    "tailwind.config.js"
  ]
}
